	"name": "mdscript",
	"displayName": "Minecraft Datapack Script",
	"description": "Create in a more common way a minecraft datapack",
	"version": "0.0.1",
	"author": {
		"name": "BlaXrew Studios",
		"url": "http://www.blaxrew-studios.vision",
		"email": "blaxrew.studios@gmail.com"
	},
	"icon": "https://lh3.googleusercontent.com/pw/ACtC-3cEDdxdlVEPQHFLOhTtPmbmuNJt2IjpPkTkP6Gj9IdZRO69cZ57ixyU0CtBd_DEVfGnS5U0OHPnpxE63vFY5dkfda944KoZptE8-UBKGXo9iOhocdxByvnGvWhaUUqeC1g82WiVA_mlHtrZW5LMD4C5=w748-h593-no?authuser=0",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Themes",
		"Programming Languages",
		"Snippets"
	],
	"contributes": {
		"themes": [
			{
				"label": "MD Script Theme",
				"uiTheme": "vs-dark",
				"path": "./themes/MD Script Theme-color-theme.json"
			}
		],
		"languages": [
			{
				"id": "mdscript",
				"aliases": [
					"MD Script",
					"mdscript"
				],
				"extensions": [
					".mds",
					".mdscript"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"snippets": [
			{
				"language": "mdscript",
				"path": "./snippets.json"
			}
		],
		"grammars": [
			{
				"language": "mdscript",
				"scopeName": "source.mdscript",
				"path": "./syntaxes/mdscript.tmLanguage.json"
			}
		],
		"semanticTokenTypes": [
			{
				"id": "templateType",
				"superType": "type",
				"description": "A template type."
			}
		],
		"semanticTokenModifiers": [
			{
				"id": "native",
				"description": "Annotates a symbol that is implemented natively"
			}
		]
	},
	"scripts": {
		"compile": ".\\compile.js"
	}
}
